{
  "Vendor": true,
  "Deadline": "2m",
  "Sort": [
    "linter",
    "severity",
    "path",
    "line"
  ],
  "Exclude": [
    "should have signature WriteByte\\(byte\\) error \\(vet\\)",
    "should have signature ReadByte\\(\\) \\(byte, error\\) \\(vet\\)",
    "should have comment or be unexported \\(golint\\)",
    "consts.go.*should have comment",
    "rc",
    "test/compatibility",
    "test\\\\compatibility",
    "internal/proto",
    "internal\\\\proto",
    "sample/orgwebsite",
    "sample\\\\orgwebsite",
    "internal/common/murmur.go",
    "internal\\\\common/murmur.go",
    ".*DataOffset should have comment.*",
    ".*TypePortable should have comment.*",
    ".*LifecycleStateStarting should have comment.*",
    ".*aggregator_hook.go.* is unused*",
    "address can be fmt.Stringer \\(interfacer\\)"
  ],
  "EnableGC": true,
  "WarnUnmatchedDirective": true,
  "DisableAll": true,
  "Enable": [
    "deadcode",
    "gofmt",
    "goimports",
    "golint",
    "gosimple",
    "ineffassign",
    "interfacer",
    "lll",
    "misspell",
    "unconvert",
    "unparam",
    "unused",
    "vet"
  ],
  "LineLength": 130
}